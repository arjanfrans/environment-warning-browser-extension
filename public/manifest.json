{
  "name": "Environment Warnings",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["*://*/*"],
  "incognito": "spanning",
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "favicon"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
