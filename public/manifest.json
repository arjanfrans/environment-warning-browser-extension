{
  "name": "Environment Warning",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "optional_host_permissions": ["*://*/*"],
  "incognito": "spanning",
  "optional_permissions": [
    "scripting"
  ],
  "permissions": [
    "storage",
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html"
}
